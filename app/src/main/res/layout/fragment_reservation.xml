<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/titreclub"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="16dp"
        android:text="@string/URL_addCard"
        android:textColor="@color/greenAlbaColor"
        android:textSize="20sp"
        android:textStyle="bold" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/icone1"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_marginLeft="16dp"
            android:backgroundTint="@color/colorPrimary"
            android:tint="@color/colorPrimary"
            app:srcCompat="@drawable/resa" />

        <TextView
            android:id="@+id/date"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:paddingLeft="8dp"
            android:text="TextView"
            android:textColor="@color/colorPrimary"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/icone2"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_marginLeft="16dp"
            android:backgroundTint="@color/colorPrimary"
            android:tint="@color/colorPrimary"
            app:srcCompat="@drawable/ic_horloge" />

        <TextView
            android:id="@+id/date2"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:paddingLeft="8dp"
            android:text="TextView"
            android:textAlignment="textStart"
            android:textColor="@color/colorPrimary"
            android:textSize="14sp" />

    </LinearLayout>

    <TextView
        android:id="@+id/date3"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimary"
        android:textSize="14sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/date4"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="TextView"
            android:textColor="@color/colorPrimary"
            android:textSize="14sp"
            tools:text="@string/mesResas_name" />

        <TextView
            android:id="@+id/date5"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:layout_weight="1"
            android:paddingLeft="8dp"
            android:text="TextView"
            android:textAlignment="textStart"
            android:textColor="@color/colorPrimary"
            android:textSize="14sp"
            android:textStyle="bold" />

    </LinearLayout>

    <View
        android:id="@+id/separator1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray" />


</LinearLayout>